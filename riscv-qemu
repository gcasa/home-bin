#!/bin/sh

qemu-system-riscv64 -device VGA,edid=on,xres=1680,yres=1050   -machine virt   -smp 4   -m 2G   -kernel /Volumes/Data/Projects/RISC-V/Fedora-Developer-Rawhide-20200108.n.0-fw_payload-uboot-qemu-virt-smode.elf   -bios none   -object rng-random,filename=/dev/urandom,id=rng0   -device virtio-rng-device,rng=rng0   -device virtio-blk-device,drive=hd0   -drive file=/Volumes/Data/Projects/RISC-V/Fedora-33-Rawhide-Kernel-5.14.16-101.0.riscv64.fc33.riscv64-20211109.n.0-sda.raw,format=raw,id=hd0   -device virtio-net-device,netdev=usernet -device virtio-mouse-pci -device virtio-keyboard-pci  -netdev user,id=usernet,hostfwd=tcp::10000-:22